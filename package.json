{
  "name": "catala",
  "displayName": "Catala",
  "description": "A VSCode extension and LSP server for the Catala language",
  "version": "1.1.0",
  "publisher": "catalalang",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CatalaLang/catala-language-server/"
  },
  "icon": "logo.png",
  "categories": [
    "Programming Languages"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "catala_en",
        "path": "./snippets/catala_en.json"
      },
      {
        "language": "catala_fr",
        "path": "./snippets/catala_fr.json"
      }
    ],
    "commands": [
      {
        "command": "catala.selectScope",
        "title": "Select scope"
      },
      {
        "command": "catala.run",
        "icon": "$(play)",
        "title": "Run a scope"
      },
      {
        "command": "catala.debug",
        "icon": "$(debug-alt)",
        "title": "Debug a scope"
      },
      {
        "command": "catala.openWithTestEditor",
        "title": "%command.catala.openWithTestEditor.title%",
        "category": "%category.catala%",
        "icon": "$(open-preview)"
      },
      {
        "command": "catala.openInputEditor",
        "title": "Scope Input Editor"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "catala.openWithTestEditor",
          "when": "config.catala.enableCustomTestCaseEditor && resourceFilename =~ /.*test.*\\.catala_(en|fr)$/",
          "group": "navigation@2"
        }
      ],
      "editor/title": [
        {
          "command": "catala.run",
          "when": "editorLangId == catala_fr || editorLangId == catala_en",
          "group": "navigation@10"
        },
        {
          "command": "catala.debug",
          "when": "editorLangId == catala_fr || editorLangId == catala_en",
          "group": "navigation@11"
        },
        {
          "command": "catala.openWithTestEditor",
          "when": "config.catala.enableCustomTestCaseEditor && (editorLangId == catala_en || editorLangId == catala_fr) && resourceFilename =~ /.*test.*\\.catala_(en|fr)$/",
          "group": "navigation@2"
        }
      ]
    },
    "breakpoints": [
      {
        "language": "catala_fr"
      },
      {
        "language": "catala_en"
      }
    ],
    "debuggers": [
      {
        "type": "catala-debugger",
        "label": "Catala Debug",
        "languages": [
          "catala_en",
          "catala_fr"
        ],
        "variables": {
          "SpecifyScope": "catala.selectScope"
        },
        "configurationAttributes": {
          "launch": {
            "required": [
              "scope"
            ],
            "properties": {
              "args": {
                "type": "object",
                "description": "Catala Debug arguments",
                "properties": {
                  "file": {
                    "type": "string",
                    "description": "Path of a Catala program.",
                    "default": ""
                  },
                  "scope": {
                    "type": "string",
                    "description": "Scope name in the Catala program.",
                    "default": ""
                  }
                },
                "additionalProperties": false
              },
              "stopOnEntry": {
                "type": "boolean",
                "description": "Automatically stop after launch.",
                "default": true
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "catala-debugger",
            "request": "launch",
            "name": "Catala Debug",
            "args": "${command:catala.selectScope}",
            "stopOnEntry": true
          }
        ],
        "configurationSnippets": [
          {
            "label": "Catala Debug: Launch",
            "description": "New configuration for debugging a Catala program",
            "body": {
              "type": "catala-debugger",
              "name": "Catala debug (Launch)",
              "request": "launch",
              "args": "^\"\\${command:SpecifyScope}\""
            },
            "trace": true
          }
        ]
      }
    ],
    "configuration": {
      "title": "Catala",
      "properties": {
        "catala.enableCustomTestCaseEditor": {
          "type": "boolean",
          "default": false,
          "description": "Enable the custom test case editor for Catala test files"
        },
        "catala-lsp.trace.server": {
          "description": "Controls the logging output of the language server. Valid settings are `off`, `messages`, or `verbose`.",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "compact",
            "verbose"
          ],
          "default": "messages"
        },
        "catala-lsp.enableProjectScan": {
          "description": "Controls whether the LSP server should actively scan through the project to detect dependency breakage. This may be ressource-intensive on large projects. Switching this parameter requires a reload of VSCode to become effective.",
          "type": "boolean",
          "default": "true"
        },
        "catala.lspServerPath": {
          "type": "string",
          "default": null,
          "description": "Custom path to the `catala-lsp` executable"
        },
        "catala.catalaFormatPath": {
          "type": "string",
          "default": null,
          "description": "Custom path to the `catala-format` executable"
        },
        "catala.catalaPath": {
          "type": "string",
          "default": null,
          "description": "Custom path to the `catala` executable"
        },
        "catala.clerkPath": {
          "type": "string",
          "default": null,
          "description": "Custom path to the `clerk` executable"
        }
      }
    },
    "customEditors": [
      {
        "viewType": "catala.testCaseEditor",
        "displayName": "%customEditor.testCaseEditor.displayName%",
        "selector": [
          {
            "filenamePattern": "*test*.catala_*"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "catala.scopeInputEditor",
        "displayName": "Scope Input Editor",
        "selector": []
      }
    ],
    "languages": [
      {
        "id": "catala_en",
        "aliases": [
          "Catala (English)"
        ],
        "extensions": [
          ".catala_en",
          ".catala_en.md"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "catala_fr",
        "aliases": [
          "Catala (French)"
        ],
        "extensions": [
          ".catala_fr",
          ".catala_fr.md"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "catala_type_en",
        "configuration": "./language-configuration.json"
      },
      {
        "id": "catala_type_fr",
        "configuration": "./language-configuration.json"
      },
      {
        "id": "catala_code_en",
        "configuration": "./language-configuration.json"
      },
      {
        "id": "catala_code_fr",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "catala_en",
        "scopeName": "source.catala_en",
        "path": "./syntaxes/en.xml"
      },
      {
        "language": "catala_fr",
        "scopeName": "source.catala_fr",
        "path": "./syntaxes/fr.xml"
      },
      {
        "language": "catala_type_en",
        "scopeName": "source.catala_type_en",
        "path": "./syntaxes/types_en.xml"
      },
      {
        "language": "catala_type_fr",
        "scopeName": "source.catala_type_fr",
        "path": "./syntaxes/types_fr.xml"
      },
      {
        "language": "catala_code_en",
        "scopeName": "source.catala_code_en",
        "path": "./syntaxes/code_en.xml"
      },
      {
        "language": "catala_code_fr",
        "scopeName": "source.catala_code_fr",
        "path": "./syntaxes/code_fr.xml"
      }
    ],
    "comments": {
      "lineComment": "#"
    },
    "configurationDefaults": {
      "[catala_en]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
      },
      "[catala_fr]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
      }
    }
  },
  "activationEvents": [
    "workspaceContains:**/clerk.toml",
    "workspaceContains:**/*.catala_en",
    "workspaceContains:**/*.catala_fr"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "opam exec -- dune build @lsp; webpack --mode development",
    "test": "tsc && vscode-test",
    "webview:test": "node node_modules/vitest/vitest.mjs run",
    "webview:test:watch": "node node_modules/vitest/vitest.mjs",
    "format": "prettier --write .",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.ts,.tsx --fix",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "prepare": "husky",
    "atd:generate_ts": "cd src/generated && atdts ../../catala-atd/catala_types.atd",
    "check:css": "node purgecss-check.js",
    "knip": "knip",
    "knip:exports": "knip --include exports --no-progress"
  },
  "dependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode-webview": "^1.57.5",
    "@vscode/codicons": "^0.0.36",
    "command-exists": "^1.2.9",
    "p-queue": "^6.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.1.9",
    "vscode-languageclient": "^8.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/command-exists": "^1.2.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.19.1",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.15.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.14.0",
    "glob": "^11.0.3",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "knip": "^5.71.0",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.3",
    "purgecss": "^7.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,json}": "npm run format",
    "src/**/*.{js,ts,jsx,tsx}": "npm run lint:fix",
    "src/styles/*.css": "node lint_scripts/purgecss-check.js",
    "src/locales/*.json": "node lint_scripts/i18n.js",
    "src/**/*.tsx": "node lint_scripts/i18n.js"
  }
}
